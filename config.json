{
  "name": "ngrok Client",
  "version": "1.1",
  "slug": "ngrok",
  "description": "A ngrok client for Hass.io",
  "webui": "http://[HOST]:[PORT:4040]",
  "url": "https://github.com/ThePicklenat0r/hassio-ngrok",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "ports": {
    "4040/tcp": "4040"
  },
  "ports_description": {
    "4040/tcp": "Web interface"
  },
  "options": {
    "auth_token": "",
    "region": "us",
    "tunnels": [
      {
        "proto": "http",
        "addr": "172.30.33.2:8123",
        "inspect": false,
        "bind-tls": "both"
      }
    ]
  },
  "schema": {
    "NGROK_AUTH": "str?",
    "NGROK_REGION": "list(us|eu|ap|au|sa|jp|in)?",
    "TUNNELS": [
      {
        "proto": "list(http|tls|tcp)?",
        "addr": "str?",
        "inspect": "bool?",
        "auth": "str?",
        "host_header": "str?",
        "bind-tls": "list(true|false|both)?",
        "subdomain":  "str?",
        "hostname": "str?",
        "crt": "str?",
        "key": "str?",
        "client_cas": "str?",
        "remote_addr": "str?",
        "metadata": "str?"
      }
    ]
  },
  "image": "thepicklenat0r/{arch}-hassio-ngrok"
}